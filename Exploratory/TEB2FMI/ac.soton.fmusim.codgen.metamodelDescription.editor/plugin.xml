<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="FmiModel.presentation.FmiModelModelWizardID"
            name="%_UI_FmiModelModelWizard_label"
            class="FmiModel.presentation.FmiModelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/FmiModelModelFile.gif">
         <description>%_UI_FmiModelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="FmiModelV1.presentation.FmiModelEditorID"
            name="%_UI_FmiModelEditor_label"
            icon="icons/full/obj16/FmiModelModelFile.gif"
            extensions="fmimodelv1"
            class="FmiModelV1.presentation.FmiModelV1Editor"
            contributorClass="FmiModelV1.presentation.FmiModelV1ActionBarContributor">
      </editor>
   </extension>

</plugin>
